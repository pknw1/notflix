          "_name": "design-block",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"accordion3\" group=\"Accordion & Toggles & Tabs\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-switch-arrow\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n<header>© Pratical Plugins</header>\n<input type=\"checkbox\" title=\"Show Title\" name=\"ShowAccTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showAccSubtitle\" checked>\n            <input type=\"checkbox\" title=\"Show Text\" name=\"ShowAccText\" checked>\n<input type=\"checkbox\" title=\"Show Button\" name=\"ShowButton\">\n<input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\">\n    <input type=\"color\" title=\"Color tab\" name=\"tabColor\" value=\"#2b3541\">\n<input type=\"color\" title=\"Body Color\" name=\"Bodycolor\" value=\"#e5a00d\">\n    \n      <input type=\"range\" inline title=\"Border Radius\" name=\"borderradius\" min=\"0\" max=\"50\" step=\"1\" value=\"0\">\n <input type=\"range\" inline title=\"Margin Tab\" name=\"marginbottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n\n      <input type=\"range\" inline title=\"Width Content\" name=\"widthContent\" min=\"1\" max=\"4\" step=\"1\" value=\"4\">\n    \n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <select title=\"Amount\" name=\"bootstrapAccordion\">\n             <option value=\"1\">1</option>\n             <option value=\"2\">2</option>\n             <option value=\"3\">3</option>\n             <option value=\"4\">4</option>\n             <option value=\"5\">5</option>\n             <option value=\"6\" selected>6</option>\n        </select>\n\n <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"6\">\n    <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"2\">\n\n