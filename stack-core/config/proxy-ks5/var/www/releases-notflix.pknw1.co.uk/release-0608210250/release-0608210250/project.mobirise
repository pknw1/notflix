          "_name": "witsec-modal-window-block",
          "_sourceTheme": "mobirise5",
          "_anchor": "witsec-modal-window-block-n",
          "modalWindow": "<div class=\"modal \" id=\"credentials\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"credentialsLabel\" aria-hidden=\"true\">  <div class=\"modal-dialog modal-sm modal-dialog-centered\" style=\"height:auto\" role=\"document\">    <div class=\"modal-content\"><div class=\"modal-body\" id=\"credentials_body\">Login anywhere using <br /><br />username: pkmovies<br />password: pknw1plex</div>    </div>  </div></div><script>document.addEventListener(\"DOMContentLoaded\", function() {  $(\"#credentials\").on(\"hidden.bs.modal\", function () {    var html = $( \"#credentials_body\" ).html();    $( \"#credentials_body\" ).empty();    $( \"#credentials_body\" ).append(html);  })});</script>"
