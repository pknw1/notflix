# php.pknw1.co.uk
upstream php.pknw1.co.uk {
				# Cannot connect to network of this container
				server 127.0.0.1 down;
}
server {
	server_name php.pknw1.co.uk;
	listen 80 ;
	access_log /var/log/nginx/access.log vhost;
	location / {
		root   /var/www/html;
		include fastcgi_params;
		fastcgi_pass php.pknw1.co.uk;
	}
}
