              },
              "z-index": "5000"
            },
            ".windowFixed": {
              "display": "none",
              "position": "fixed",
              "right": "(@FloatingFormXpos * 1px)",
              "bottom": "(@FloatingFormYpos * 1px)",
              "overflow": "auto"
            },
            ".windowBar": {
              "width": "100%",
              "background-color": "@windowBarColor",
              "color": "#fff",
              "padding": "5px",
              "border-top-left-radius": "(@floatingFormBorderRadius * 1px) !important",
              "border-top-right-radius": "(@floatingFormBorderRadius * 1px) !important",
              "border-bottom-left-radius": "0px !important",
              "border-bottom-right-radius": "0px !important"
